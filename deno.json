{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ dev.ts",
    "update-fresh": "deno run -A -r https://fresh.deno.dev/update .",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts",
    "astro": "astro dev",
    "start:api": "deno run --allow-net --allow-read api.ts",
    "watch:api": "deno run --watch --allow-net --allow-read api.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.7.3/",
    "@/": "./",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.1",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.2",
    "@twind/aspect-ratio": "https://esm.sh/v98/@twind/aspect-ratio@0.1.4",
    "@twind/core": "https://esm.sh/@twind/core@1.1.3",
    "@twind/preset-autoprefix": "https://esm.sh/@twind/preset-autoprefix@1.0.7/",
    "@twind/preset-tailwind": "https://esm.sh/@twind/preset-tailwind@1.1.4/",
    "astro": "npm:astro@^5.13.7",
    "hono": "jsr:@hono/hono@^4.9.7",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.2.2",
    "preact": "https://esm.sh/preact@10.22.0",
    "preact/": "https://esm.sh/preact@10.22.0/",
    "twind": "https://esm.sh/twind@0.16.19",
    "twind/": "https://esm.sh/twind@0.16.19/"
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "deploy": {
    "project": "4e5cf50c-11c3-4701-b052-3188465fec02",
    "exclude": [
      "**/node_modules"
    ],
    "include": [],
    "entrypoint": "main.ts"
  }
}
